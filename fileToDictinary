def main():
    try:
        StudentList = {}
        fileInput = open("Studentsinfo.txt", 'r')
        students = fileInput.readlines()
        fileInput.close()
        for s in students:
            parts = s.split(',')
            StudentList[parts[0]] = [parts[1], parts[2], parts[3], parts[4]]
        print(StudentList)
    except FileNotFoundError:
        print("File Not Found")
    main()